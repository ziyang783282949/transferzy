<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SimpleSession">

	<resultMap id="BaseResultMap" type="SimpleSession">
		<id column="id" jdbcType="INTEGER" property="id" />
		<result column="cookie" jdbcType="VARCHAR" property="cookie" />
		<result column="session" jdbcType="VARCHAR" property="session" />
		
	</resultMap>

	<insert id="addEntity" parameterType="java.util.Map">
		insert into 
		tb_session(
			cookie,session
		)values(
			#{cookie},#{session}
		)
	</insert>

	<select id="findEntityListByCond" parameterType="java.util.Map" resultMap="BaseResultMap">
	
		select * from tb_session
		<trim prefix="where" prefixOverrides="and" >
			
			<if test="id != null">
				and id=#{id} 
			</if>
			<if test="cookie != null">
				and cookie=#{cookie} 
			</if>
			<if test="session != null">
				and session=#{session} 
			</if>
		</trim>
		
	</select>
	
	<update id="updateEntity" parameterType="java.util.Map">
		update tb_session
		<trim suffixOverrides = ",">
		<set>
			<if test="cookie != null">
				cookie=#{cookie},
			</if>
			<if test="session != null">
				session=#{session},
			</if>
		</set>
		</trim>
		where id = #{id}
	</update>
	
	<delete id="deleteEntity" parameterType="java.util.Map">
		delete from tb_session
		<trim prefix="where" prefixOverrides="and">
			<if test="id != null">
				and id=#{id}
			</if>
			<if test="cookie != null">
				and cookie=#{cookie}
			</if>
			<if test="session != null">
				and session=#{session}
			</if>
		</trim>
	</delete>
	
	
</mapper>